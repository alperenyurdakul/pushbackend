# Twilio SMS Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here
TWILIO_VERIFY_SERVICE_SID=your_twilio_verify_service_sid_here

# OneSignal Push Notifications
ONESIGNAL_APP_ID=e4150da6-cd3a-44f2-a193-254898ba5129
ONESIGNAL_APP_AUTH_KEY=os_v2_app_4qkq3jwnhjcpfimtevejrosrff6ayktoxlcusomf7x2kzt473d74prkgsjciz5qfn4zaef4i5isfvk7omc2aihyycxfrbxosoudl2bq

# Firebase Cloud Messaging (FCM) - Android Push Notifications
# Firebase Console > Project Settings > Service Accounts > Generate New Private Key
# JSON dosyasını indir ve tüm içeriğini buraya ekle (tek satır olarak)
# VEYA base64 encode edip buraya ekle
FIREBASE_SERVICE_ACCOUNT_KEY={"type":"service_account","project_id":"your-project-id",...}

# Apple Push Notification Service (APNs) - iOS Push Notifications
# Apple Developer > Certificates, Identifiers & Profiles > Keys > Create a key
# Key ID ve Team ID'yi al, .p8 dosyasını indir
APNS_KEY_ID=your_apns_key_id_here
APNS_TEAM_ID=your_apns_team_id_here
# .p8 dosyasının içeriğini buraya ekle (-----BEGIN PRIVATE KEY----- ile başlayan)
APNS_KEY=-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----
APNS_BUNDLE_ID=com.faydana.alperen
# Production: true | Development: false
APNS_PRODUCTION=true

# AI Service

# Server Configuration
PORT=5000
NODE_ENV=development